
@{
    ViewBag.Title = "List";
}
@model IEnumerable<WebApplication2.Models.Product>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    
</head>
<body>
    @foreach (var item in Model)
    {

        <h3>Имя продукта: @item.Name</h3>
        <p>ID продукта: @item.Id</p>
        <p>Стоимость продукта: @item.Price</p>
        <p>Описание продукта: @item.Description</p>

    }
</body>
</html>

